// Appsettings is renamed because of an issue where the project loaded another appsettings.json

{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft": "Warning",
            "Microsoft.Hosting.Lifetime": "Information"
        }
    },
    "Config": {
        "JobSchedulerConfig": {
            // Gets expected graduation users between now and time range (in months).
            "TimeRange": 6,
            "TimeBetweenJobsInMs": 10000
        },
        "ApiConfig": {
            "ApiUrl": "https://localhost:5001/"
        },
        "IdentityServerConfig": {
            "IdentityUrl": "https://localhost:5005/",
            "ClientId": "dex-jobscheduler",
            "ClientSecret": "dex-jobscheduler"
        }
    }
}
